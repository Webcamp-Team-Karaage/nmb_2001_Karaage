<div class="row">
  <div class="col-md-6 col-md-offset-3">
<%= form_for(resource, as: resource_name, url: session_path(resource_name), class: "form-horizontal") do |f| %>
  <div class="field form-group">
    <%= f.label :email, "メールアドレス", class: "col-md-3 control-label" %>
    <%= f.email_field :email, autofocus: true, autocomplete: "email", class: "col-md-9" %>
  </div>

  <div class="field form-group">
    <%= f.label :password, "パスワード", class: "col-md-3 control-label" %>
    <%= f.password_field :password, autocomplete: "current-password", class: "col-md-9" %>
  </div>

  <!-- <% if devise_mapping.rememberable? %> -->
    <!-- <div class="field"> -->
      <!-- <%= f.check_box :remember_me %> -->
      <!-- <%= f.label :remember_me %> -->
    <!-- </div> -->
  <!-- <% end %> -->

  <div class="actions form-group">
    <%= f.submit "ログイン", class: "btn btn-primary col-md-offset-2 col-md-10" %>
  </div>
<% end %>

 <!-- <%= render "admins/shared/links" %> -->
  </div>
</div>