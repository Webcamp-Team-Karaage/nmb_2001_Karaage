<h4>登録情報編集</h4>

<%= form_for(@member) do |f| %>

<%= link_to "退会する", delete_me_path %>

<h5>氏名</h5>
<%= f.text_field :first_name %>
<%= f.text_field :last_name %>
<h5>カナ</h5>
<%= f.text_field :first_name_kana %>
<%= f.text_field :last_name_kana %>
<h5>郵便番号</h5>
<%= f.text_field :postal_code %>
<h5>電話番号</h5>
<%= f.text_field :phone %>
<h5>メールアドレス</h5>
<%= f.email_field :email %>

<%= f.submit "編集内容を保存する" %>


<% end %>